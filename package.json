{
  "name": "bosnian_company_root",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "install-all": "npm --prefix bosnia-companies-ai/backend install && npm --prefix bosnia-companies-ai/frontend install",
    "dev": "npm-run-all -p dev:backend dev:frontend",
    "dev:backend": "npm --prefix bosnia-companies-ai/backend run dev",
    "dev:frontend": "npm --prefix bosnia-companies-ai/frontend run dev",
    "build": "npm --prefix bosnia-companies-ai/frontend run build",
    "start": "npm --prefix bosnia-companies-ai/backend run start"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}